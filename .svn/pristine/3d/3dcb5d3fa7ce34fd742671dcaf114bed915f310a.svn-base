#import "APPDBManage.h"

@implementation APPDBManage

+ (APPDBManage *)sharedSingleton
{
    static APPDBManage *sharedSingleton;
    
    @synchronized(self)
    {
        if (!sharedSingleton)
            sharedSingleton = [[APPDBManage alloc] init];
        return sharedSingleton;
    }
}

@end
